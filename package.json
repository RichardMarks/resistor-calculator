{
  "name": "resistor-calculator",
  "displayName": "Resistor Calculator",
  "version": "1.0.0",
  "description": "A simple resistor calculator",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "@material-ui/core": "^4.8.3",
    "@material-ui/icons": "^4.5.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "cordova": "^9.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && cp -a ./build/. app/ResistorCalculator/www/",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "cordova-osx": "cd app/ResistorCalculator && cordova prepare osx && cordova run osx",
    "cordova-android": "cd app/ResistorCalculator && cordova prepare android && cordova run android"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
